{
    "name": "tatikoma/react-cassandra",
    "type": "library",
    "description": "Performant pure-PHP CQL (Cassandara) async (ReactPHP) library",
    "keywords": [
        "cassandra",
        "cql",
        "react-cassandra",
        "react",
        "react-php",
        "reactphp"
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Tatikoma"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "react/event-loop": "~0.4",
        "react/promise": "~2.2",
        "react/stream": "^0.4.4",
      "evenement/evenement": "^2.0",
      "clue/block-react": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "React\\Cassandra\\": [
                "src/react/cassandra/"
            ]
        },
        "classmap": [
            "src/react/cassandra/"
        ]
    }
}
